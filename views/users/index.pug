h1 Users

a(href="/users/create") Create

form(action="/users/search", method="GET")
	input(type="text", name="q")
	button Search

ul
	each user in users
		li= "Name :" + user.name + " - Age : " + user.age
		a(href="/users/edit/" + user.id) Edit
		span= "----"
		a(href="/users/delete/" + user.id) Delete